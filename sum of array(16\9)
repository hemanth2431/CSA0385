#include<stdio.h>
int sumOfArrays(int num1[],int m,int num2[],int n) {
    int totalSum=0;
    for(int i=0;i<m;i++) {
        totalSum+=num1[i];
    }
    for(int i=0;i<n;i++) {
        totalSum+=num2[i];
    }
    return totalSum;
}
int main() 
{
    int num1[]={1,2,3,4,5};
    int num2[]={ 6,7,8,9,10};
    int m=sizeof(num1)/sizeof(num1[0]);
    int n=sizeof(num2)/sizeof(num2[0]);
    int result=sumOfArrays(num1,m,num2,n);
    printf("The sum of the two arrays is: %d",result);
    return 0;
}
